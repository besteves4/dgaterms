    <!-- http://anon/dgaterms#connectedWith -->

    <owl:ObjectProperty rdf:about="http://anon/dgaterms#connectedWith">
        <rdfs:domain rdf:resource="http://anon/dgaterms#DataUser"/>
        <rdfs:range rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://anon/dgaterms#hasSingleInformationPoint -->

    <owl:ObjectProperty rdf:about="http://anon/dgaterms#hasSingleInformationPoint">
        <rdfs:domain rdf:resource="http://anon/dgaterms#PublicSectorBody"/>
        <rdfs:range rdf:resource="http://anon/dgaterms#SingleInformationPoint"/>
        <rdfs:comment xml:lang="en">Indicates the responsible single information point entity</rdfs:comment>
        <rdfs:label xml:lang="en">has single information point</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://anon/dgaterms#hasPublicSectorBody -->

    <owl:ObjectProperty rdf:about="http://anon/dgaterms#hasPublicSectorBody">
        <rdfs:range rdf:resource="http://anon/dgaterms#PublicSectorBody"/>
        <rdfs:comment xml:lang="en">Indicates association with public sector body</rdfs:comment>
        <rdfs:label xml:lang="en">has public sector body</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://anon/dgaterms#hasCompetentBody -->

    <owl:ObjectProperty rdf:about="http://anon/dgaterms#hasCompetentBody">
        <rdfs:range rdf:resource="http://anon/dgaterms#CompetentBody"/>
        <rdfs:comment xml:lang="en">Indicates association with competent body</rdfs:comment>
        <rdfs:label xml:lang="en">has competent body</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://anon/dgaterms#hasDataUser -->

    <owl:ObjectProperty rdf:about="http://anon/dgaterms#hasDataUser">
        <rdfs:range rdf:resource="http://anon/dgaterms#DataUser"/>
        <rdfs:comment xml:lang="en">Indicates association with data user</rdfs:comment>
        <rdfs:label xml:lang="en">has data user</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://anon/dgaterms#hasDataHolder -->

    <owl:ObjectProperty rdf:about="http://anon/dgaterms#hasDataHolder">
        <rdfs:range rdf:resource="http://anon/dgaterms#DataHolder"/>
        <rdfs:comment xml:lang="en">Indicates association with data holder</rdfs:comment>
        <rdfs:label xml:lang="en">has data holder</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://anon/dgaterms#hasDataAltruismOrganisation -->

    <owl:ObjectProperty rdf:about="http://anon/dgaterms#hasDataAltruismOrganisation">
        <rdfs:range rdf:resource="http://anon/dgaterms#DataAltruismOrganisation"/>
        <rdfs:comment xml:lang="en">Indicates association with data altruism organisation</rdfs:comment>
        <rdfs:label xml:lang="en">has data altruism organisation</rdfs:label>
    </owl:ObjectProperty>



    <!-- http://anon/dgaterms#hasThirdCountryJurisdiction -->

    <owl:ObjectProperty rdf:about="http://anon/dgaterms#hasThirdCountryJurisdiction"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://anon/dgaterms#connectedWith -->

    <owl:DatatypeProperty rdf:about="http://anon/dgaterms#connectedWith"/>
    


    <!-- http://anon/dgaterms#hasActivityStartingDate -->

    <owl:DatatypeProperty rdf:about="http://anon/dgaterms#hasActivityStartingDate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <dct:source>DGA 11.6.g</dct:source>
        <rdfs:comment xml:lang="en">Indicates the date when an entity starts its activity</rdfs:comment>
        <rdfs:label xml:lang="en">has activity starting date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://anon/dgaterms#hasFee -->

    <owl:DatatypeProperty rdf:about="http://anon/dgaterms#hasFee">
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <dct:source>DGA 2.10</dct:source>
        <rdfs:comment xml:lang="en">Indicates the fee charged by an entity to provide a service</rdfs:comment>
        <rdfs:label xml:lang="en">has fee</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://anon/dgaterms#hasProcessingDate -->

    <owl:DatatypeProperty rdf:about="http://anon/dgaterms#hasProcessingDate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <dct:source>DGA 20.1.b</dct:source>
        <rdfs:comment xml:lang="en">Indicates the date when an entity starts to process data</rdfs:comment>
        <rdfs:label xml:lang="en">has processing date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://anon/dgaterms#isFreeOfCharge -->

    <owl:DatatypeProperty rdf:about="http://anon/dgaterms#isFreeOfCharge">
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <dct:source>DGA 2.10</dct:source>
        <rdfs:comment xml:lang="en">Indicates if the service provided by an entity is free of charge</rdfs:comment>
        <rdfs:label xml:lang="en">is free of charge</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/date -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://anon/dgaterms#AggregatedData -->

    <owl:Class rdf:about="http://anon/dgaterms#AggregatedData">
        <dct:source>DGA 5.3.a.ii</dct:source>
        <rdfs:comment xml:lang="en">Data that resulted from the aggregation of several datum</rdfs:comment>
        <rdfs:label xml:lang="en">Aggregated Data</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#AltruisticPurpose -->

    <owl:Class rdf:about="http://anon/dgaterms#AltruisticPurpose">
        <dct:source>DGA 2.16</dct:source>
        <rdfs:comment xml:lang="en">Purposes associated with the voluntary sharing of data for the general interest of the public, such as healthcare or combating climate change</rdfs:comment>
        <rdfs:label xml:lang="en">Altruistic Purpose</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#CombatClimateChange -->

    <owl:Class rdf:about="http://anon/dgaterms#CombatClimateChange">
        <dct:source>DGA 2.16</dct:source>
        <rdfs:comment xml:lang="en">Purposes associated with combating the causes and consequences of climate change, including reducing gas emissions and fighting emergencies such as floods or wildfires</rdfs:comment>
        <rdfs:label xml:lang="en">CombatClimateChange</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#CommercialConfidentiality -->

    <owl:Class rdf:about="http://anon/dgaterms#CommercialConfidentiality">
        <dct:source>DGA 3.1.a</dct:source>
        <rdfs:comment xml:lang="en">Data held by public sector bodies can be reused on the grounds of commercial confidentiality, including business, professional and company secrets</rdfs:comment>
        <rdfs:label xml:lang="en">Commercial Confidentiality</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#CompetentBody -->

    <owl:Class rdf:about="http://anon/dgaterms#CompetentBody">
        <dct:source>DGA 7</dct:source>
        <rdfs:comment xml:lang="en">An entity designated by a public sector body to provide legal and technical support on the access and reuse of public sector data</rdfs:comment>
        <rdfs:label xml:lang="en">CompetentBody</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#ConsentAndPermissionManagement -->

    <owl:Class rdf:about="http://anon/dgaterms#ConsentAndPermissionManagement">
        <dct:source>DGA 12.n</dct:source>
        <rdfs:comment xml:lang="en">Methods to obtain and withdraw consent or permission to process data</rdfs:comment>
        <rdfs:label xml:lang="en">Consent and Permission Management</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataAltruismOrganisation -->

    <owl:Class rdf:about="http://anon/dgaterms#DataAltruismOrganisation">
        <dct:source>DGA 2.16, 18</dct:source>
        <rdfs:comment xml:lang="en">An non-profit organisation whom collects and shares data for altruistic purposes</rdfs:comment>
        <rdfs:label xml:lang="en">Data Altruism Organisation</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataCooperativeServiceProvider -->

    <owl:Class rdf:about="http://anon/dgaterms#DataCooperativeServiceProvider">
        <dct:source>DGA 2.15, 10.c</dct:source>
        <rdfs:comment xml:lang="en">An entity constituted by data subjects, one-person undertakings or SMEs who provides data intermediation services and supports its members in the exercise of their data-related rights</rdfs:comment>
        <rdfs:label xml:lang="en">Data Cooperative Service Provider</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataExchangeMethod -->

    <owl:Class rdf:about="http://anon/dgaterms#DataExchangeMethod">
        <dct:source>DGA 12.e</dct:source>
        <rdfs:comment xml:lang="en">Methods to facilitate the exchange of data</rdfs:comment>
        <rdfs:label xml:lang="en">Data Exchange Method</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataHolder -->

    <owl:Class rdf:about="http://anon/dgaterms#DataHolder">
        <dct:source>DGA 2.8</dct:source>
        <rdfs:comment xml:lang="en">An entity who has the right to grant access to or to share certain personal data or non-personal data</rdfs:comment>
        <rdfs:label xml:lang="en">Data Holder</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataIntermediationServiceProvider -->

    <owl:Class rdf:about="http://anon/dgaterms#DataIntermediationServiceProvider">
        <dct:source>DGA 2.11</dct:source>
        <rdfs:comment xml:lang="en">An entity who establishes commercial relationships for the data sharing between data subjects and data holders on the one hand and data users on the other</rdfs:comment>
        <rdfs:label xml:lang="en">Data Intermediation Service Provider</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataIntermediationServiceProviderForDataHolder -->

    <owl:Class rdf:about="http://anon/dgaterms#DataIntermediationServiceProviderForDataHolder">
        <dct:source>DGA 10.a</dct:source>
        <rdfs:comment xml:lang="en">An entity who makes data holders&apos; data available for potential data users, including bilateral or multilateral exchanges of data and platforms and databases for the joint exploitation of data</rdfs:comment>
        <rdfs:label xml:lang="en">Data Intermediation Service Provider for Data Holder</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataIntermediationServiceProviderForDataSubject -->

    <owl:Class rdf:about="http://anon/dgaterms#DataIntermediationServiceProviderForDataSubject">
        <dct:source>DGA 10.b</dct:source>
        <rdfs:comment xml:lang="en">An entity who makes data subjects&apos; personal data available for potential data users</rdfs:comment>
        <rdfs:label xml:lang="en">Data Intermediation Service Provider for Data Subject</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataRequestProcedure -->

    <owl:Class rdf:about="http://anon/dgaterms#DataRequestProcedure">
        <dct:source>DGA 5.1</dct:source>
        <rdfs:comment xml:lang="en">Procedure to request data for reuse via single information point</rdfs:comment>
        <rdfs:label xml:lang="en">Data Request Procedure</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataSubjectRightsManagement -->

    <owl:Class rdf:about="http://anon/dgaterms#DataSubjectRightsManagement">
        <dct:source>DGA 12.m</dct:source>
        <rdfs:comment xml:lang="en">Methods to facilitate the exercising of data subjects&apos; rights</rdfs:comment>
        <rdfs:label xml:lang="en">Data Subject Rights Management</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataUser -->

    <owl:Class rdf:about="http://anon/dgaterms#DataUser">
        <dct:source>DGA 2.9</dct:source>
        <rdfs:comment xml:lang="en">An entity who has access an the right to use personal or non-personal data for commercial or non-commercial purposes</rdfs:comment>
        <rdfs:label xml:lang="en">Data User</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#Display -->

    <owl:Class rdf:about="http://anon/dgaterms#Display">
        <dct:source>DGA 2.20</dct:source>
        <rdfs:comment xml:lang="en">Show data in an organised way so that it can be seen by the public</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/odrl/2/display"/>
    </owl:Class>
    


    <!-- http://anon/dgaterms#Download -->

    <owl:Class rdf:about="http://anon/dgaterms#Download">
        <dct:source>DGA 2.13, 2.20</dct:source>
        <rdfs:comment xml:lang="en">Copy data from one computer system to another over a network, e.g., over the internet</rdfs:comment>
        <rdfs:label xml:lang="en">Download</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#EuropeanDataInnovationBoard -->

    <owl:Class rdf:about="http://anon/dgaterms#EuropeanDataInnovationBoard">
        <dct:source>DGA 29</dct:source>
        <rdfs:comment xml:lang="en">An authority tasked with overseeing the activities of data intermediation service providers and data altruism organisations</rdfs:comment>
        <rdfs:label xml:lang="en">European Data Innovation Board</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#DataIntermediationAuthority -->

    <owl:Class rdf:about="http://anon/dgaterms#DataIntermediationAuthority">
        <dct:source>DGA 13</dct:source>
        <rdfs:comment xml:lang="en">An authority tasked with overseeing the activity of data intermediation service providers and maintaining a public register of said entities</rdfs:comment>
        <rdfs:label xml:lang="en">Data Intermediation Authority</rdfs:label>
    </owl:Class>



    <!-- http://anon/dgaterms#DataAltruismAuthority -->

    <owl:Class rdf:about="http://anon/dgaterms#DataAltruismAuthority">
        <dct:source>DGA 23</dct:source>
        <rdfs:comment xml:lang="en">An authority tasked with overseeing the activity of data altruism organisations and maintaining a public register of said entities</rdfs:comment>
        <rdfs:label xml:lang="en">Data Altruism Authority</rdfs:label>
    </owl:Class>



    <!-- http://anon/dgaterms#ImproveHealthcare -->

    <owl:Class rdf:about="http://anon/dgaterms#ImproveHealthcare">
        <dct:source>DGA 2.16</dct:source>
        <rdfs:comment xml:lang="en">Purposes associated with improving the public healthcare systems across the Member States, including improving personalised treatments and helping cure chronic diseases</rdfs:comment>
        <rdfs:label xml:lang="en">Improve Healthcare</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#ImproveMobility -->

    <owl:Class rdf:about="http://anon/dgaterms#ImproveMobility">
        <dct:source>DGA 2.16</dct:source>
        <rdfs:comment xml:lang="en">Purposes associated with improving traffic, public transport systems or costs for drivers</rdfs:comment>
        <rdfs:label xml:lang="en">Improve Mobility</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#Licence -->

    <owl:Class rdf:about="http://anon/dgaterms#Licence">
        <dct:source>DGA 2.10</dct:source>
        <rdfs:comment xml:lang="en">Document which states the conditions for usage of data</rdfs:comment>
        <rdfs:label xml:lang="en">Licence</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/odrl/2/Offer"/>
    </owl:Class>
    


    <!-- http://anon/dgaterms#ModifiedData -->

    <owl:Class rdf:about="http://anon/dgaterms#ModifiedData">
        <dct:source>DGA 5.3.a.ii</dct:source>
        <rdfs:comment xml:lang="en">Data that is updated in relation to its original state</rdfs:comment>
        <rdfs:label xml:lang="en">Modified Data</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#ProvideOfficialStatistics -->

    <owl:Class rdf:about="http://anon/dgaterms#ProvideOfficialStatistics">
        <dct:source>DGA 2.16</dct:source>
        <rdfs:comment xml:lang="en">Purposes associated with facilitating the development, production and dissemination of reliable official statistics</rdfs:comment>
        <rdfs:label xml:lang="en">Provide Official Statistics</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#ProvidePublicServices -->

    <owl:Class rdf:about="http://anon/dgaterms#ProvidePublicServices">
        <dct:source>DGA 2.16</dct:source>
        <rdfs:comment xml:lang="en">Purposes associated with improving the provision of public services, such as public safety, education or law enforcement</rdfs:comment>
        <rdfs:label xml:lang="en">Provide Public Services</rdfs:label>
    </owl:Class>
    


    <!-- http://anon/dgaterms#PublicPolicyMaking -->

    <owl:Class rdf:about="http://anon/dgaterms#PublicPolicyMaking">
        <dct:source>DGA 2.16</dct:source>
        <rdfs:comment xml:lang="en">Purposes associated with public policy making, such as the development of new laws</rdfs:comment>
