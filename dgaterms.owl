    <!-- http://anon/dgaterms#conversionFormat -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#conversionFormat">
        <dct:source>DGA 12.d</dct:source>
        <rdfs:comment xml:lang="en">The formats to which the data is allowed to be converted</rdfs:comment>
        <rdfs:label xml:lang="en">conversion format</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/format"/>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#Data"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasForm -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasForm">
        <dct:source>DGA 11.6.b</dct:source>
        <rdfs:comment xml:lang="en">Indicates the form of an entity</rdfs:comment>
        <rdfs:label xml:lang="en">has form</rdfs:label>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasLegalStatus -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasLegalStatus">
        <dct:source>DGA 11.6.b</dct:source>
        <rdfs:comment xml:lang="en">Indicates the legal status of an entity</rdfs:comment>
        <rdfs:label xml:lang="en">has legal status</rdfs:label>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasOwnershipStructure -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasOwnershipStructure">
        <dct:source>DGA 11.6.b</dct:source>
        <rdfs:comment xml:lang="en">Indicates the ownership structure of an entity</rdfs:comment>
        <rdfs:label xml:lang="en">has ownership structure</rdfs:label>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasRegistrationNumber -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasRegistrationNumber">
        <dct:source>DGA 11.6.b</dct:source>
        <rdfs:comment xml:lang="en">Indicates the registration number of an entity</rdfs:comment>
        <rdfs:label xml:lang="en">has registration number</rdfs:label>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasSourceOfIncome -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasSourceOfIncome">
        <dct:source>DGA 19.4.d</dct:source>
        <rdfs:comment xml:lang="en">Indicates sources of income of the entity</rdfs:comment>
        <rdfs:label xml:lang="en">has source of income</rdfs:label>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasSourceOfRevenue -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasSourceOfRevenue">
        <dct:source>DGA 20.2.e</dct:source>
        <rdfs:comment xml:lang="en">Indicates sources of revenue of the entity</rdfs:comment>
        <rdfs:label xml:lang="en">has source of revenue</rdfs:label>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasStatute -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasStatute">
        <dct:source>DGA 19.4.c</dct:source>
        <rdfs:comment xml:lang="en">Indicates the statute of the entity</rdfs:comment>
        <rdfs:label xml:lang="en">has statute</rdfs:label>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasSubsidiary -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasSubsidiary">
        <dct:source>DGA 11.6.b</dct:source>
        <rdfs:comment xml:lang="en">Indicates a subsidiary of an entity</rdfs:comment>
        <rdfs:label xml:lang="en">has subsidiary</rdfs:label>
        <rdfs:domain rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://anon/dgaterms#hasThirdCountryJurisdiction -->

    <owl:AnnotationProperty rdf:about="http://anon/dgaterms#hasThirdCountryJurisdiction">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv#hasJurisdiction"/>
        <rdfs:range rdf:resource="https://w3id.org/dpv#Location"/>
    </owl:AnnotationProperty>
    


