<?xml version="1.0"?>
<rdf:RDF xmlns="http://anon/dgaterms#"
     xml:base="http://anon/dgaterms"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:dpv="https://w3id.org/dpv#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
