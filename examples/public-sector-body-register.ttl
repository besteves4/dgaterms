PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX dpv: <https://w3id.org/dpv#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX ex: <http://example.com/>
PREFIX : <http://anon/dgaterms#>

ex:publicsectorbody1_RA_catalog a dcat:Catalog ;
    dct:description "Catalog of registers of activities of Public Sector Body 1" ;
    dct:created "2022-12-03"^^xsd:date ;
    dct:publisher ex:publicsectorbody1 ;
    dcat:dataset ex:publicsectorbody1_RA_1, ex:requestForAggregatedData .

ex:publicsectorbody1 a :PublicSectorBody ;
    dpv:hasName "Public Sector Body 1" ;
    dpv:hasContact "mailto:publicsectorbody1@email.com" .

ex:publicsectorbody1_RA_1 a :RegisterOfPublicSectorBodyActivity ;
    dct:description "Register of activity of Public Sector Body 1" ;
    dct:created "2022-12-04"^^xsd:date ;
    :hasPublicSectorBody ex:publicsectorbody1 ;
    :hasCompetentBody ex:competentbody1 ;
    dpv:hasData ex:publicsectordata1 ;
    dpv:hasPurpose dpv:ResearchAndDevelopment ;
    odrl:hasPolicy ex:policy1 ;
    :isFreeOfCharge "true"^^xsd:boolean ;
    dcat:mediaType <https://www.iana.org/assignments/media-types/text/csv> ;
    dct:extent "5.6MB"^^xsd:string .

ex:competentbody1 a :CompetentBody ;
    dpv:hasName "Competent Body 1" ;
    dpv:hasContact "mailto:competentbody1@email.com" .

ex:publicsectordata1 a :AggregatedData ;
    rdfs:label "Example of aggregated data that follows under DGA's Article 3." .

ex:policy1 a odrl:Offer ;
    odrl:uid <http://example.com/policy1> ;
    rdfs:comment "Policy set by the a PublicSectorBody, http://example.com/publicsectorbody1, that permits the use of the target dataset http//example.com/publicsectordata1 until the end of the year 2023."@en ;
    odrl:permission [
        odrl:target ex:publicsectordata1 ;
        odrl:assigner ex:publicsectorbody1 ;
        odrl:action odrl:use ;
        odrl:constraint [
            odrl:leftOperand odrl:dateTime ;
            odrl:operator odrl:lteq ;
            odrl:rightOperand "2023-12-31"^^xsd:date
        ]
    ] .

ex:requestForAggregatedData a :DataRequestProcedure ;
    :hasDataUser ex:datauser1 ;
    dpv:hasData ex:publicsectordata1 ;
    dpv:hasPurpose dpv:ResearchAndDevelopment .

ex:datauser1 a :DataUser ;
    dpv:hasName "Data User 1" ;
    dpv:hasContact "mailto:datauser1@email.com" .