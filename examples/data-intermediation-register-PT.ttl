PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX ex: <http://example.com/>
PREFIX : <http://anon/dgaterms#>

ex:publicregistry_DI_PT a :PublicRegisterOfDataIntermediationServiceProviders ;
    dct:description "Public register of intermediaries working in PT" ;
    dct:created "2023-12-15"^^xsd:date ;
    dct:publisher ex:nationalauthority_PT ;
    :hasDataIntermediationServiceProvider ex:DISP_1 .

ex:nationalauthority_PT a :DataIntermediationAuthority ;
    dpv:hasName "Data Intermediation Authority of Portugal" ;
    dpv:hasContact "mailto:nationalauthority_PT@email.com" ;
    dpv:hasJurisdiction "PT" .

ex:DISP_1 a :DataIntermediationServiceProvider ;
    dpv:hasName "Data Intermediation Service Provider 1" ;
    dct:description "Provider of anonymised geolocation data" ;
    :hasDataIntermediationServiceProviderType :DataCooperativeServiceProvider ;
    :hasLegalStatus ex:legalStatusA ;
    :hasForm ex:formA ;
    :hasOwnershipStructure ex:structureA ;
    :hasSubsidiary ex:subsidiaryA ;
    :hasRegistrationNumber "125689"^^xsd:integer ;
    dpv:hasAddress "Lisboa, Portugal" ;
    dcat:landingPage <http://example.com/DISP1> ;
    dct:issued "2023-12-23"^^xsd:date ;
    :hasActivityStartingDate "2024-01-01"^^xsd:date .